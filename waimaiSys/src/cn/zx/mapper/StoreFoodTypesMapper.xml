<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.zx.mapper.StoreFoodTypesMapper">
    
     <!-- 查询所有商家 -->
    <select id="findStoreFoodTypes" parameterType="Integer" resultType="StoreFoodTypes" >
    	SELECT * FROM store_foodtypes sf INNER JOIN food_types ft WHERE ft.`id`=sf.`foodTypeId` AND sf.`storeId`=#{storeId}
    </select>
    
</mapper>